{
    "name": "@engineer-dao/contracts",
    "version": "0.1.0",
    "description": "Contracts",
    "main": "dist/index",
    "types": "dist/index",
    "scripts": {
        "compile": "hardhat compile",
        "deploy": "hardhat --network testnet deploy",
        "deploy-prod": "hardhat --network mainnet deploy",
        "testc": "clear & hardhat test",
        "build": "yarn build:contracts && yarn && yarn build:typescript",
        "build:typescript": "tsc -p ./tsconfig.json",
        "build:contracts": "hardhat compile --show-stack-traces",
        "test": "yarn test:contracts",
        "test:contracts": "hardhat test --show-stack-traces",
        "test:coverage": "NODE_OPTIONS=--max_old_space_size=8192 hardhat coverage && istanbul check-coverage --statements 88 --branches 76 --functions 84 --lines 88",
        "test:slither": "slither .",
        "pretest:slither": "rm -f @openzeppelin && rm -f @ens && rm -f hardhat && ln -s ../../node_modules/@openzeppelin @openzeppelin && ln -s ../../node_modules/@ens @ens && ln -s ../../node_modules/hardhat hardhat",
        "posttest:slither": "rm -f @openzeppelin && rm -f @ens && rm -f hardhat",
        "lint:ts:check": "eslint . --max-warnings=0",
        "lint:contracts:check": "yarn solhint -f table 'contracts/**/*.sol'",
        "lint:check": "yarn lint:contracts:check && yarn lint:ts:check",
        "lint:ts:fix": "eslint --fix .",
        "lint:contracts:fix": "yarn prettier --write 'contracts/**/*.sol'",
        "lint:fix": "yarn lint:contracts:fix && yarn lint:ts:fix",
        "lint": "yarn lint:fix && yarn lint:check",
        "clean": "rm -rf ./dist ./artifacts ./cache ./coverage ./tsconfig.build.tsbuildinfo",
        "validateDocs": "hardhat validateOutput"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/"
    },
    "keywords": [],
    "author": "Indeavr",
    "dependencies": {
        "ethers": "^5.4.5",
        "@ethersproject/providers": "^5.0.17",
        "@flashbots/ethers-provider-bundle": "^0.2.1",
        "@tenderly/hardhat-tenderly": "^1.0.12",
        "@types/decimal.js": "^7.4.0",
        "@types/paralleljs": "0.0.20",
        "abi-decoder": "^2.4.0",
        "axios": "^0.21.1",
        "bignumber.js": "^9.0.1",
        "decimal.js": "^10.2.1",
        "dotenv": "^8.2.0",
        "node-schedule": "^2.0.0",
        "ws": "^7.4.4",
        "@eth-optimism/core-utils": "0.7.3",
        "@ethersproject/abstract-provider": "^5.4.1",
        "@ethersproject/abstract-signer": "^5.4.1",
        "@ethersproject/hardware-wallets": "^5.4.0"
    },
    "devDependencies": {
        "@nomiclabs/hardhat-ethers": "npm:hardhat-deploy-ethers@^0.3.0-beta.7",
        "@nomiclabs/hardhat-etherscan": "^2.1.0",
        "@nomiclabs/hardhat-waffle": "^2.0.1",
        "@typechain/ethers-v5": "^5.0.0",
        "@types/chai": "^4.2.18",
        "@types/lodash": "^4.14.168",
        "@types/mkdirp": "^1.0.1",
        "@types/mocha": "^8.2.2",
        "@typechain/hardhat": "^3.0.0",
        "@types/node": "^14.14.11",
        "chai": "^4.3.4",
        "mocha": "^8.4.0",
        "ethereum-waffle": "^3.3.0",
        "ethers": "^5.1.4",
        "hardhat": "^2.3.0",
        "hardhat-deploy": "^0.9.3",
        "hardhat-gas-reporter": "^1.0.4",
        "hardhat-output-validator": "^0.1.17",
        "hardhat-typechain": "^0.3.3",
        "ts-generator": "^0.1.1",
        "ts-node": "^9.1.1",
        "typechain": "^4.0.1",
        "typescript": "^4.1.2",
        "solhint": "^3.3.6",
        "solhint-plugin-prettier": "^0.0.5",
        "solidity-coverage": "^0.7.17",
        "prettier": "^2.3.1",
        "prettier-plugin-solidity": "^1.0.0-beta.18",
        "lodash": "^4.17.20",
        "lint-staged": "11.0.0",
        "eslint": "^7.27.0",
        "eslint-config-prettier": "^8.3.0",
        "eslint-plugin-ban": "^1.5.2",
        "eslint-plugin-import": "^2.23.4",
        "eslint-plugin-jsdoc": "^35.1.2",
        "eslint-plugin-prefer-arrow": "^1.2.3",
        "eslint-plugin-prettier": "^3.4.0",
        "eslint-plugin-react": "^7.24.0",
        "eslint-plugin-unicorn": "^32.0.1",
        "@typescript-eslint/eslint-plugin": "^4.26.0",
        "@typescript-eslint/parser": "^4.26.0",
        "@openzeppelin/contracts": "4.3.2",
        "@openzeppelin/contracts-upgradeable": "4.3.2",
        "@primitivefi/hardhat-dodoc": "^0.1.3",
        "@codechecks/client": "^0.1.11",
        "babel-eslint": "^10.1.0"
    }
}
